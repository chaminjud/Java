package Sec08;

public class ArrayAddExam {

	public static void main(String[] args) {
		
		String[] members = new String[2];
		String[] membersB = {"안철수", "김영희"}; 
		
		members[0] = "철수";
		members[1] = "영희";
		
		for(int i =0 ; i < 2 ; i++) {
			System.out.println("멤버명단 :" + members[i]);
			System.out.println("멤버명단 :" + membersB[i]);
		}
		
//      공간을 만들어 두면 일반적으로 더 추가 불가능 배열 복사기능 사용
