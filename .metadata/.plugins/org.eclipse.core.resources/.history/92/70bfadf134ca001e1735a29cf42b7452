package Sec06;

public class ArrayCreateExam {

	public static void main(String[] args) {
		
		//배열을 선언과 동시에 생성
         String[] season = {"String","Summer","fall","winter"};
         
         season[1] = "여름";
         
         for(int i =0 ; i < season.length; i++) {
        	 System.out.println(season[i]);
         }
	}

}
