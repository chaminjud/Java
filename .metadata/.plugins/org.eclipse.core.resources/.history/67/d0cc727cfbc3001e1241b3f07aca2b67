package sec04;

public class BreakLabelExam {

	public static void main(String[] args) {
		Outer: for(char upper='A'; upper <= 'Z'; upper++) {
			for(char lower = 'a'; lower <= 'z'; lower++) {
				System.out.println(upper + "-" + lower);
				if(lower == 'g') {
					break Outer;
					
					//레이블 Outer를 이용해서 만든 for문 
				}
			}
		}

	}

}
