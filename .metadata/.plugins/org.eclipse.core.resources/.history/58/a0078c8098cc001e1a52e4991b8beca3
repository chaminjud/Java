package Sec06;

public class Car {

	
	//필드
	
	String company = "현대자동차";
	String model;
	String color;
	int speed;
	
	//생성자 오버로딩
	Car(){} // 매개변수에 값이 없을때
   Car(String model){ //매개변수에 값이 1개
	   this.model = model;
   }
   Car(String model, String color){//매개변수에 값이 2개
	   this.model = model;
	   this.color = color;
	   
   }
   Car(String model, String color,int speed){//매개변수에 값이 3개
	   this.model = model;
	   this.color = color;
	   this.speed = speed;
	   
	   
		//생성자 오버로딩 
		//메소드 오버로딩
		// 문법은 동일하다 생성자에 쓰거나 메소드에 쓰는 차이
		
		//함수의 매개변수가 있을때
//		function(a,b,c){
//			
//		}
//		매개변수가 있을수도 있고 없을 수도 있을때 사용한다
		
//		str();
//		
//		str(a);
//		
//		str(a,b);
//		
//		str(a,b,c);
//		
//		str(a,b,c);
	   
   }
}
