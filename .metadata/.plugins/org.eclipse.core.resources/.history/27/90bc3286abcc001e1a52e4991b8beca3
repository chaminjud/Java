package Sec06;

import java.util.Scanner;

public class Calculator2 {

	Scanner sc = new Scanner(System.in);
	int x;
	int y;
	
	
	Calculator2(){}
	
	Calculator2(int a, int b){
	}
	
	
	
	
     int add(int x ,int y) {
		int rs = x + y;
		return rs;
	}
	
	int minus(int x , int y) {
		int rs2 = x - y;
		return rs2;
	}
	
	
	
	
	void SwitchOn(){
		System.out.println("전원을 켰습니다.");
	}
	
	void SwitchOff() {
		System.out.println("전원을 껏습니다.");
	}
}
