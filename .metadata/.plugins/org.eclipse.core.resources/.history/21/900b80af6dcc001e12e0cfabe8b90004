package Sec04;

public class Test01 {

	public static void main(String[] args) {
		
		
		
		
		int a = 74;
		int b = 36;
		
		System.out.println(a+b);
		
		int c = 50;
		System.out.println(c/3);
		
		
		System.out.println("------------------");
	    int num = -5 + 8 * 6;
	    int num2 = (55+9) %9;
	    double num3 =(20 + 3*5/8);
	    int num4 = 5+15/3*2-8%3;
	    
	    System.out.println(num);
	    System.out.println(num2);
	    System.out.println(num3);
	    System.out.println(num4);
	    
	    
	    for(int i = 1 ; i <= 99; i++) {
	    	for(int z = 1 ; z <= 9 ; z ++) {
	    		int x = i * z;
	    		System.out.println(i + "x" + z + "=" + x);
	    	}
	    	System.out.println("---------------------");
	    }
	}

}
