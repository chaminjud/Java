package Sec06;

public class ArrayCreate3Exam {

	public static void main(String[] args) {
		
		//배열 선언과 생성
		int[] arr1 = new int[3];
		
		//초기값을 세팅됨
        for(int i =0; i<3; i ++) {
        	System.out.print(arr1[i]+",");
        }
        System.out.println();
        arr1[0] = 20;
        arr1[1] = 40;
        for(int i =0; i<3; i ++) {
        	System.out.print(arr1[i]+ ",");
        }
        System.out.println();
	}

}
