package Sec05;

public class KoreanExam {

	public static void main(String[] args) {
		
		Korean k1 = new Korean("안철수", "213233-123123",65);
		Korean k2 = new Korean("이영자", "123123-212331",40);
		
		System.out.println(k1.name);
		System.out.println(k1.age);
		System.out.println(k1.ssn);
		System.out.println(k1.name + k1.ssn + k1.ssn);
		
		System.out.println(k2.name);
		System.out.println(k2.age);
		System.out.println(k2.ssn);
		System.out.println(k2.name + k2.ssn + k2.ssn);

		k1.active();
	}

}
