package Sec06;

public class Car {

	/*
	 * 매개값으로 객체의 필드를 다양하게 초기화 하는것 = 오버로딩(OverLoading)
	 * 매개변수를 달리하는 생성자를 여러개 생성하는 것을 '생성자오버로딩'이라함
	 * 매개변수의 갯수가 같을때는 타입이 달라야하고 아니면 매개변수의 개수가 달라야함
	 * 
	 */
	
	//필드
	
	String company = "현대자동차";
	String model;
	String color;
	int speed;
	
	//생성자 오버로딩
	Car(){} // 매개변수에 값이 없을때
   Car(String model){ //매개변수에 값이 1개
	   this.model = model;
   }
   Car(String model, String color){//매개변수에 값이 2개
	   this.model = model;
	   this.color = color;
	   
   }

   Car(String model, String color,int speed){//매개변수에 값이 3개
	   this.model = model;
	   this.color = color;
	   this.speed = speed;
   }
	   
		//생성자 오버로딩 
		//메소드 오버로딩
		// 문법은 동일하다 생성자에 쓰거나 메소드에 쓰는 차이
		
		//함수의 매개변수가 있을때
//		function(a,b,c){
//			
//		}
//		매개변수가 있을수도 있고 없을 수도 있을때 사용한다
		
//		str();
//		
//		str(a);
//		
//		str(a,b);
//		
//		str(a,b,c);
//		
//		str(a,b,c);
	   
	   
   
}
