package Sec05;

public class CarExam {

	public static void main(String[] args) {
		
		//객체 생성
		Car MyCar = new Car();
		
		System.out.println("모델명 :" + MyCar.model);
		System.out.println("시동여부 :" + MyCar.start);
		System.out.println("현재속도 :" + MyCar.speed);
		
		//Car 객체의 필드값 변경
		MyCar.speed = 60;
		System.out.println("수정된 속도는: " + MyCar.speed);

	}

}
